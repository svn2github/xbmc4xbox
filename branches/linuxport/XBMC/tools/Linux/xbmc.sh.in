#!/bin/sh

# Set XBMC_HOME if xbmc.bin is a symlink
if [ -L @prefix@/share/xbmc/xbmc.bin ]; then
  export XBMC_HOME="@prefix@/share/xbmc"
fi

python @prefix@/share/xbmc/FEH.py "$@"
if [ $? -ne 0 ]; then
  exit
fi
@prefix@/share/xbmc/xbmc.bin "$@"
while [ $? -eq 65 ]
do
  @prefix@/share/xbmc/xbmc.bin "$@"
done

