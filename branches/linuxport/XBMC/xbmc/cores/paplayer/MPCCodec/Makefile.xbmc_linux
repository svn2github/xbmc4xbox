#
# To build, first do ./configure and then make. The create the XBMC so by calling this makefile.
#
libmpcdec-i486-linux.so:
	$(CC) -shared -o $@ src/.libs/*.o `cat ../../DllLoader/exports/wrapper.def` ../../DllLoader/exports/wrapper.o && cp $@ ../../../../system/players/paplayer/

clean:
	$(RM) *.so
