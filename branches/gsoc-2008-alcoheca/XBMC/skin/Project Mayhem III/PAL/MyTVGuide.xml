					<height>32</height>
					<font>special12</font>
					<aligny>center</aligny>
					<selectedcolor>green</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="image">
					<width>32</width>
					<height>32</height>
					<posx>1r</posx>
					<posy>1r</posy>
					<info>ListItem.Icon</info>
				</control>
			</focusedchannellayout>
			<itemlayout height="34" width="40">
				<control type="image" id="1">
					<width>40</width>
					<height>34</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>dark-green.png</texture>
					<bordertexture>totalblack.png</bordertexture>
					<bordersize>1,1,0,0</bordersize>
				</control>
				<control type="label" id="2">
					<posx>6</posx>
					<posy>3</posy>
					<width>30</width>
					<height>22</height>
					<font>font13</font>
					<aligny>center</aligny>
					<selectedcolor>green</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="image">
					<width>15</width>
					<height>14</height>
					<posx>0r</posx>
					<posy>0r</posy>
					<texture>OverlayRecording.png</texture>
					<visible>ListItem.Property(Recording)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="34" width="40">
				<control type="image" id="1">
					<width>40</width>
					<height>34</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>light-green.png</texture>
					<bordertexture>totalblack.png</bordertexture>
					<bordersize>1,1,0,0</bordersize>
