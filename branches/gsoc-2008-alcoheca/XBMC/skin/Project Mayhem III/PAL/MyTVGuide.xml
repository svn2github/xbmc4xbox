				<control type="label" id="2">
					<posx>6</posx>
					<posy>3</posy>
					<width>30</width>
					<height>22</height>
					<font>font13</font>
					<aligny>center</aligny>
					<selectedcolor>green</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="image">
					<width>15</width>
					<height>14</height>
					<posx>0r</posx>
					<posy>0r</posy>
					<texture>OverlayRecording.png</texture>
					<visible>ListItem.Property(Recording)</visible>
				</control>
			</focusedlayout>
		</control>
		<include>windowswitchtransition</include>
	</controls>
</window>
