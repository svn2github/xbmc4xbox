			<description>EPG Grid</description>
			<posx>20</posx>
			<posy>101</posy>
			<width>656</width>
			<height>437</height>
			<onleft>20</onleft>
			<onright>20</onright>
			<onup>20</onup>
			<ondown>20</ondown>
			<pagecontrol>20</pagecontrol>
			<scrolltime>350</scrolltime>
			<timeblocks>36</timeblocks>
			<rulerunit>3</rulerunit>
			<bordertexture>totalblack.png</bordertexture>
			<bordersize>1,1,0,1</bordersize>
			<rulerlayout height="29" width="40">
				<control type="image" id="1">
					<width>40</width>
					<height>29</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>totalblack.png</texture>
					<bordertexture>white.png</bordertexture>
					<bordersize>1,0,0,0</bordersize>
				</control>
				<control type="label" id="1">
					<posx>4</posx>`
					<posy>3</posy>
					<width>34</width>
					<height>29</height>
					<font>font13</font>
					<aligny>center</aligny>
					<selectedcolor>green</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</rulerlayout>
			<channellayout height="34" width="80">
				<animation effect="fade" start="70" time="200">UnFocus</animation>
				<control type="image" id="1">
					<width>80</width>
					<height>34</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>dark-green.png</texture>
					<bordertexture>totalblack.png</bordertexture>
					<bordersize>0,1,1,0</bordersize>
				</control>
				<control type="label" id="1">
					<posx>4</posx>
					<posy>1</posy>
					<width>72</width>
					<height>32</height>
					<font>special12</font>
					<aligny>center</aligny>
					<selectedcolor>green</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="image">
					<width>32</width>
					<height>32</height>
					<posx>1r</posx>
					<posy>2r</posy>
					<info>ListItem.Icon</info>
				</control>
			</channellayout>
			<focusedchannellayout height="34" width="80">
				<animation effect="fade" start="70" time="200">OnFocus</animation>
				<control type="image" id="1">
					<width>80</width>
					<height>34</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>dark-green.png</texture>
					<bordertexture>light-green.png</bordertexture>
					<bordersize>2,2,2,2</bordersize>
