AR = ar
ARFLAGS = rus
RM = rm -f

CXX=g++
CC=gcc

CFLAGS =-D_LINUX
CFLAGS+=-isysroot /Developer/SDKs/MacOSX10.4u.sdk -mmacosx-version-min=10.4

LIB=libxbms-osx.a

OBJS := ccbuffer.o ccutil.o ccxclient.o ccxclientconnxbox.o ccxdiscover.o ccxencode.o ccxmltrans.o

lib: $(LIB)

$(LIB): $(OBJS)
	$(AR) $(ARFLAGS) $@ $(OBJS)

clean:
	$(RM) -f $(OBJS) $(LIB)

distclean: clean

#include ../../../Makefile.include

